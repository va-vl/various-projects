{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "xo",
    "overrides": [
        {
            "extends": [
                "xo-typescript"
            ],
            "files": [
                "*.ts",
                "*.tsx"
            ]
        }
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": ["./tsconfig.json"]
    },
    "plugins": ["prettier"],
    "rules": {
      "no-console": "warn",
      "prettier/prettier": "error"
    }
}
