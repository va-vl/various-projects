{
  "name": "game-of-life",
  "version": "1.0.0",
  "description": "Typescript implementation of Conway's Game of Life",
  "source": "public/index.html",
  "scripts": {
    "cleanup": "rm -rf ./dist ./.parcel-cache ./parcel-bundle-reports",
    "start": "parcel --lazy --port 3000 --open",
    "build": "npm run cleanup && parcel build",
    "build:analyze": "npm run cleanup && parcel build --reporter @parcel/reporter-bundle-analyzer",
    "build:serve": "npm run cleanup && parcel build && serve -s ./dist",
    "lint": "eslint --ext ts,tsx src",
    "lint:fix": "eslint --ext ts,tsx src --fix"
  },
  "keywords": [],
  "author": "Vasili Zadorozhnyuk",
  "license": "ISC",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@parcel/reporter-bundle-analyzer": "^2.8.2",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-typescript": "^0.55.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "parcel": "^2.8.2",
    "prettier": "^2.8.3",
    "process": "^0.11.10",
    "serve": "^14.1.2",
    "typescript": "^4.9.4"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "@parcel/bunler-default": {
    "http": 2
  }
}
